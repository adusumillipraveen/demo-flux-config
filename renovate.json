{
    "extends": ["config:base"],
    "kubernetes": {
      "fileMatch": ["\\.yaml$"]
    },
    "flux": {
      "fileMatch": ["\\.yaml$"]
    },
    "masterIssue": true,
    "regexManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": ["ENV APP_INSIGHTS_AGENT_VERSION=(?<currentValue>.*?)\\n"],
      "datasource": "github-releases",
      "versioning": "semver" 
    }
  ]
  }
