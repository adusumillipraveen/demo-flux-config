{
    "extends": ["config:base"],
    "kubernetes": {
      "fileMatch": ["\\.yaml$"]
    },
    "flux": {
      "fileMatch": ["\\.yaml$"]
    },
    "masterIssue": true,
   "regexManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchStrings": [
        "ARG .*?_VERSION=(?<currentValue>.*)\\s"
      ]
     }
    ]
  }
